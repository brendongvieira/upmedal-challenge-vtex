    "props": {
      "tabId": "palmeiras",
      "label": "palmeiras",
      "defaultActiveTab": false
    }
  },

  "tab-list.item#4": {
    "props": {
      "tabId": "signos",
      "label": "signos",
      "defaultActiveTab": false
    }
  },

  "tab-list.item#5": {
    "props": {
      "tabId": "divertidos",
      "label": "divertidos",
      "defaultActiveTab": false
    }
  },

  "tab-list.item#6": {
    "props": {
      "tabId": "maravilhas",
      "label": "7 maravilhas",
      "defaultActiveTab": false
    }
  },

  "tab-content": {
    "children": [
      "tab-content.item#promocao",
      "tab-content.item#corinthians",
      "tab-content.item#palmeiras",
      "tab-content.item#signos",
      "tab-content.item#divertidos",
      "tab-content.item#maravilhas"
