      "tab-list.item#5",
      "tab-list.item#6"
    ]
  },

  "tab-list.item#1": {
    "props": {
      "tabId": "promocao",
      "label": "promoção",
      "defaultActiveTab": true
    }
  },

  "tab-list.item#2": {
    "props": {
      "tabId": "corinthians",
      "label": "corinthians",
      "defaultActiveTab": false
    }
  },

