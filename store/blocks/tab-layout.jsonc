  "tab-content.item#divertidos": {
    "children": ["list-context.product-list#divertidos"],
    "props": {
      "tabId": "divertidos"
    }
  },
  "tab-content.item#maravilhas": {
    "children": ["list-context.product-list#maravilhas"],
